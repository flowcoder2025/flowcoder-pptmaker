{
  "buildCommand": "prisma generate && next build",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 60,
      "includeFiles": "node_modules/.prisma/client/**"
    }
  },
  "env": {
    "PRISMA_CLI_BINARY_TARGETS": "rhel-openssl-3.0.x",
    "PRISMA_QUERY_ENGINE_BINARY": "node_modules/.prisma/client/libquery_engine-rhel-openssl-3.0.x.so.node"
  }
}
